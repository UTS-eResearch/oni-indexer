    "fs-extra": "^8.1.0",
    "hasha": "^5.0.0",
    "lodash": "^4.17.15",
    "ocfl": "^1.0.3",
    "prompts": "^2.3.0",
    "random-date-generator": "^1.0.2",
    "random-word": "^2.0.0",
    "ro-crate": "^1.2.12",
    "uuid": "^3.3.3",
    "winston": "^3.2.1",
    "yargs": "^13.3.0"
